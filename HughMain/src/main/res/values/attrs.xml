<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="uibutton">
        <attr name="imageNormal" format="reference|color" />
        <attr name="imageFocus" format="reference|color" />
        <attr name="textNormalColor" format="color" />
    	<attr name="textFocusColor" format="color" />
    </declare-styleable>
    
    	<!-- 自定义属性集 -->
	<declare-styleable name="FlyInMenu">
	    <attr name="animationDuration" format="integer" />
	    <attr name="content" format="reference"/>
	</declare-styleable>
	
	<declare-styleable name="slideswitch">
        <attr name="themeColor" format="color" />
        <attr name="isOpen" format="boolean" />
        <attr name="shape">
            <enum name="rect" value="1" />
            <enum name="circle" value="2" />
        </attr>
    </declare-styleable>
    
</resources>